[user]
	name = Jason Stubbs
[log]
	decorate = true
[core]
	excludesfile = /Users/jstubbs/.gitignore_global
[push]
	defult = upstream
	default = upstream
[diff]
    tool = vimdiff

[color]
    branch = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[url "git@bitbucket.org:"]
    insteadOf = https://bitbucket.org/
